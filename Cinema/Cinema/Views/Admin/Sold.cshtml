@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<div>
    <h3>Расписание сеансов</h3>


    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>Название фильма</th>
                <th>Зал</th>
                <th>Цена</th>
                <th>Время</th>
                <th>Место</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Cinema.Models.CinemaModels.FilmSession b in ViewBag.Data)
            {
            <tr>
                <td>@b.Film.Name </td>
                <td>@b.Session.Hall</td>
                <td>@b.Session.Price</td>
                <td>@b.Session.Time</td>
                <td>@b.Session.SeatNumber</td>
            </tr>
            }
        </tbody>
    </table>
</div>